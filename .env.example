# Supabase Configuration
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your-anon-key
SUPABASE_SERVICE_KEY=your-service-role-key

# Core Supabase Configuration
VITE_CORE_SUPABASE_URL=https://your-core-project.supabase.co
VITE_CORE_SUPABASE_ANON_KEY=your-core-anon-key

# n8n Integration
N8N_WEBHOOK_URL=https://your-n8n-instance.com/webhook/your-webhook-id/chat
N8N_API_KEY=your-n8n-api-key-for-incoming-messages
WEBHOOK_TIMEOUT_MS=120000
WEBHOOK_RETRY_ATTEMPTS=3

# Server Configuration
WEBSOCKET_PORT=8080
WEBSOCKET_PATH=/ws
CORS_ALLOWED_ORIGINS=http://localhost:5173,https://yourdomain.com

# Security
ADMIN_API_KEY=your-admin-api-key-for-cache-management
USER_RATE_LIMIT=10
API_RATE_LIMIT=100

# Cache Configuration
CACHE_TTL_MS=1800000
CACHE_STALE_MS=300000
CACHE_MAX_ITEMS=2000

# Session Configuration
HEARTBEAT_INTERVAL_MS=30000
SESSION_TIMEOUT_MS=300000

# Media Configuration
ALLOWED_MEDIA_TYPES=image/jpeg,image/png,image/gif,video/mp4,video/webm
MAX_MEDIA_SIZE=10485760

# Logging
LOG_LEVEL=info

# UI Configuration
VITE_CHANNEL_UI=chat
